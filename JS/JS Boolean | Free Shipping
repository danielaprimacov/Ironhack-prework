//const cartTotal = 50;
const cartTotal = 101;

// First if statement:
// checks if the value of cartTotal is greater than 50
if (cartTotal > 50) {
  console.log("You got Free Shipping!");
}

// Second if statement:
// checks if the value of cartTotal is smaller then or equal to 50
if (cartTotal <= 50) {
  console.log("Free shipping available on orders above 50");
}
